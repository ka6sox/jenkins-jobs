if [ -d shr-core ] ; then
  cd shr-core;
  du -hs sstate-cache
  openembedded-core/scripts/sstate-cache-management.sh -L --cache-dir=sstate-cache -d -y || true
  du -hs sstate-cache
  mkdir old || true
  umount tmp-eglibc || true
  mv -f cache/bb_codeparser.dat* bitbake.lock pseudodone tmp-eglibc* old || true
  rm -rf old
fi
echo "Cleanup finished"
